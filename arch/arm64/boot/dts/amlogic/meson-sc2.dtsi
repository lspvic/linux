	aml_mkl {
		compatible = "amlogic,aml_mkl";
		dev_name = "aml_mkl_dev";
		status = "okay";
		reg = <0x0 0xfe440200 0x0 0x80>;
	};

	chosen {
		kaslr-seed = <0x0 0x0>;
	};
